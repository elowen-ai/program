{
  "version": "0.1.7",
  "namespace": "@elowen-ai",
  "name": "@elowen-ai/program",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "main": "dist/app/index.cjs",
  "module": "dist/app/index.es.js",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/app/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/app/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    },
    "./node": {
      "types": "./dist/app/index.d.ts",
      "default": "./dist/cjs/index.js"
    },
    "./browser": {
      "types": "./dist/app/index.d.ts",
      "default": "./dist/esm/index.js"
    }
  },
  "scripts": {
    "build": "tsx esbuild.ts",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "yarn build",
    "vanity": "cd vanity && sh find.sh",
    "lint:fix": "prettier */*.js \"*/**/*{.js,.ts}\" -w",
    "lint": "prettier */*.js \"*/**/*{.js,.ts}\" --check",
    "build-devnet": "anchor build -- --features devnet",
    "build-mainnet": "anchor build",
    "deploy-devnet": "yarn build-devnet && anchor deploy",
    "deploy-mainnet": "yarn build-mainnet && anchor deploy --provider.cluster mainnet",
    "set-devnet": "solana config set --url https://api.devnet.solana.com",
    "set-mainnet": "solana config set --url https://api.mainnet-beta.solana.com",
    "show-rent": "sh -c 'solana rent $(stat -f %z target/deploy/elowen.so)'",
    "close-devnet": "solana program close --keypair ~/.config/solana/id.json --bypass --url devnet",
    "renew-buffers": "solana program close --keypair ~/.config/solana/id.json --buffers --url devnet"
  },
  "dependencies": {
    "@coral-xyz/anchor": "^0.31.0",
    "@metaplex-foundation/mpl-token-metadata": "^3.4.0",
    "@metaplex-foundation/umi": "^1.2.0",
    "@metaplex-foundation/umi-bundle-defaults": "^1.2.0",
    "@pythnetwork/pyth-solana-receiver": "^0.10.1",
    "@solana/spl-token": "^0.4.13",
    "@solana/web3.js": "^1.98.2",
    "@sqds/multisig": "^2.1.4",
    "axios": "^1.9.0",
    "nodemailer": "^7.0.3",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/bn.js": "^5.1.0",
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.0.0",
    "chai": "^4.3.4",
    "child_process": "^1.0.2",
    "dotenv": "^16.5.0",
    "esbuild": "^0.25.3",
    "esbuild-plugin-copy": "^2.1.1",
    "esbuild-plugin-tsc": "^0.5.0",
    "mocha": "^9.0.3",
    "prettier": "^2.6.2",
    "ts-mocha": "^10.0.0",
    "typescript": "^5.7.3"
  }
}
